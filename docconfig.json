{
  "opts": {
    "readme": "README.md",
    "destination": "./Docs/"
  },
  "plugins": [],
  "recurseDepth": 10,
  "source": {
    "include": [ "amt-rcs.js", "wsserver.js" ],
    "exclude": [],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [ "jsdoc", "closure" ]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}